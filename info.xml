<?xml version="1.0"?>
<extension key="com.skvare.profilecondition" type="module">
  <file>profilecondition</file>
  <name>Profile Field Conditions</name>
  <description>Set default values, conditional logic, and field visibility for profile fields on contribution pages and events</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>FIXME</author>
    <email>FIXME</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">http://FIXME</url>
    <url desc="Documentation">http://FIXME</url>
    <url desc="Support">http://FIXME</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2025-07-21</releaseDate>
  <version>1.0</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.81</ver>
  </compatibility>
  <comments>Profile field conditions and default values for online forms</comments>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Profilecondition</namespace>
    <format>23.02.1</format>
    <angularModule>crmProfilecondition</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
    <mixin>menu-xml@1.0.0</mixin>
  </mixins>
  <upgrader>CRM_Profilecondition_Upgrader</upgrader>
</extension>
